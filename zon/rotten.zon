dilbegin wombel_dil();
var
  pcname : string;
  item   : unitptr;
  pc     : unitptr;
  exdp   : extraptr;
  i      : integer;

code
{
   :init:
   heartbeat := PULSE_SEC*9;
   interrupt(SFB_COM, self.position == POSITION_FIGHTING, ohno_combat);
   on_activation(self.position <= POSITION_SLEEPING, waitawhile);

   :start:
   wait(SFB_CMD, (activator.type == UNIT_ST_PC) and (not command(CMD_GIVE)));

   if (WOMBEL_COMPLETE in activator.quests)
     goto waitawhile;

   pc := activator;
   secure(pc, lostpc);

   if (not (WOMBEL_ONGOING in pc.quests))
   {
      pause;
      act("A small cloud of dust appears in front of the bard.",
A_SOMEONE, pc, null, null, TO_ALL);
      pause;
      act("A small wombel exits the cloud " +
      "and looks at the bard.",
A_SOMEONE, pc, null, null, TO_ALL);
      pause;
      act("The small wombel whisper something to the bard " +
      "as it takes his dagger.",
A_SOMEONE, pc, null, null, TO_ALL);
      pause;
      act("Suddenly the wombel jumps back into the cloud " +
      "and with a puuufff the cloud disappears!.",
 A_SOMEONE, pc, null, null, TO_ALL);
      pause;
      exec("say Oh no, the wombel got my weapon!", self);
      pause;

      exec("say It is the only weapon that works properly against them!",
self);

      pause;
      exec("say The wombel has undoubtedly given the dagger to the Shadow Dragon " +
      "since, the Shadow Dragon is the only one "+
      "who has the power to destroy it!", self);
      pause;
      exec("say I must get it back before the dragon destroys it, "+
    "can you help me, "+pc.name+"?", self);
      i := 0;
      while (i < 5)
      {
	 wait(SFB_CMD, activator #= pc);

	 if (command(CMD_NOD))
	 {
	    goto quest_accepted;
	 }
	 else if (command(CMD_SAY))
	 {
	    pause;
	    exec("say Please nod if you accept or shake if you reject.",self);
	 }
	 else if (command(CMD_SHAKE))
	   goto no_quest;
	 i := i+1;
      };

      :no_quest:
      pause;
      exec("say I am doomed!!" +
      " the world is doomed!!", self);
      unsecure(pc);
      goto waitawhile;

      :quest_accepted:
      pause;
      exec("say Good but hurry, find the dragon before it is to late!", self);
      exec("say The dragon is somewhere in the sewers or the catacombs!", self);
      addextra(pc.quests, {WOMBEL_ONGOING}, "");
      unsecure(pc);
      goto start;
   }
   else               /* WOMBEL_ONGOING in pc.quests! */
   {
      exdp := WOMBEL_ONGOING in pc.quests;

      if (("dagger" in exdp.names))
      {
	 pause;

       subextra(pc.quests, WOMBEL_ONGOING);
	 addextra(pc.quests, {WOMBEL_COMPLETE}, "");
	 item := load("belt@rotten"); /* the prize for finishing*/
         item.height := pc.height;
	 link(item, pc);
	 experience(2000, pc);
	 exec("say You got it back! " +
	 "and I had given up on you!:)",self);
	 pause;
	exec("say That was indeed brave done!! " +
	"as a reward I shall give you this black belt. " +
	"Use it well on your adventures.", self);
	 pause;
	 exec("say You are a true defender of justice "+pc.name, self);
	 pause;
      }
   };
   unsecure(pc);
   goto start;

 :ohno_combat:
goto start;

   :waitawhile:
   heartbeat := PULSE_SEC*60;
   while ((self.position == POSITION_FIGHTING) or
	  (self.position <= POSITION_SLEEPING))
   pause;
   goto init;

   :lostpc:
   exec("say Noone will help me and I am lost without my dagger!", self);
   goto start;
}
dilend

dilbegin wombel2_dil();
var
  pc     : unitptr;
  item   : unitptr;
  exdp   : extraptr;

code
{
   :init:
   heartbeat := PULSE_SEC*5;
   on_activation(self.position <= POSITION_SLEEPING, iamout);

   :start:
   wait(SFB_CMD, (command(CMD_GIVE) and (activator.type == UNIT_ST_PC)));

   :give:
   if (not (WOMBEL_ONGOING in activator.quests))
     goto start;

   pc := activator;
   item := self.inside;
   secure(pc, labsecure);
   secure(item, labsecure);

   wait(SFB_CMD | SFB_TICK, TRUE);

    if (not (item #= self.inside))
   {

      unsecure(item);
      exdp := WOMBEL_ONGOING in pc.quests;

      if ("wombel slayer" in self.inside.name)  /*this is what you want them to give you*/
      {
	 addstring(exdp.names, "dagger");
	 exec("say It looks like my dagger!", self);
	 destroy(self.inside);
      }
      else
      {
	 exec("say But it was the wrong dagger!!:(", self);
	 link(self.inside,pc);
      };

   };
   unsecure(pc);

   if (command(CMD_GIVE) and activator.type == UNIT_ST_PC)
   {
      goto give;
   }
   else
     goto start;

   :labsecure:
   exec("say where did they go?", self);
   goto start;

   :iamout:
   heartbeat := PULSE_SEC*60;
   while (self.position <= POSITION_FIGHTING)
     pause;
   goto init;
}
dilend
end
